{
	"name": "Snapblocks",
	"version": "2.1.0",
	"author": [
		"ErnieParke",
		"blob8108",
		"apple502j",
		"kenny2github",
		"ego-lay_atman-bay",
		"Radman Siddiki"
	],
	"url": "https://github.com/snap-blocks/mw-snapblocks",
	"descriptionmsg": "snap-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"SnapBlocksHook": "SnapblocksHooks.php"
	},
	"ResourceModules": {
		"ext.snapblocks": {
			"scripts": [
				"run_snapblocks.js",
				"lib/snapblocks.min.js",
				"lib/translations-all.js"
			],
			"styles": "/inline.css"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "mw-snapblocks"
	},
	"Hooks": {
		"ParserFirstCallInit": "main",
		"ResourceLoaderGetConfigVars": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "SnapBlocksHook"
		}
	},
	"MessagesDirs": {
		"SnapBlocks": [
			"i18n"
		]
	},
	"config": {
		"SnapBlocksLangs": {
			"value": []
		},
		"SnapBlocksBlockVersion": {
			"value": "snap"
		},
		"SnapBlocksWrap": {
			"value": true
		},
		"SnapBlocksWrapSize": {
			"value": null
		},
		"SnapBlocksZebraColoring": {
			"value": true
		},
		"SnapBlocksShowSpaces": {
			"value": true
		},
		"SnapBlocksSantaHats": {
			"value": false
		}
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.40.0"
	}
}
